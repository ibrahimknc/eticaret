	<script type="text/javascript">
		var binderObj = [];
		binderObj.push(function () {
			vba.settings.serverDate = new Date(@Html.Raw(Json.Serialize(DateTimeOffset.UtcNow)));
			vba.settings.offset = @(
				TimeZoneInfo.Local.GetUtcOffset(DateTime.UtcNow).TotalMinutes
				);
			vba.render = function (item) { return function (tok, i) { if (i % 2 && tok.length) { var val = ""; try { tok = tok.split("&gt;").join(">"); val = eval(tok); } catch (e) { } return val; } else { return tok; }; }; };
